                        end
                    end
                end)
            end
        end
    end
end)
local a,b
local posi = 5
spawn(function()
    while task.wait() do
        if MyRealEnemies == nil then target() end
        if MyRealEnemies == nil then hopserver = true end 
        if MyRealEnemies.Character:FindFirstChild("HumanoidRootPart") and game.Players.LocalPlayer.Character:FindFirstChild("HumanoidRootPart") then
            if game.Players.LocalPlayer.Character:FindFirstChild("Humanoid").Health > getgenv().Setting.Another.Health then
                pcall(function()    
                    if not (game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1") and MyRealEnemies:DistanceFromCharacter(game:GetService("Workspace")["_WorldOrigin"].Locations:FindFirstChild("Island 1").Position) < 10000) then
                        if (MyRealEnemies.Character:WaitForChild("HumanoidRootPart").CFrame.Position - game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame.Position).Magnitude < 40 then 
                            if game:GetService("Players").LocalPlayer.PlayerGui.Main.SafeZone.Visible == true then
                                print("Safe Zone")
                                SkipPlayer()
                            end
                            if MyRealEnemies.Character.Humanoid.Health > 0 then
                                Click()
                                if cocc then
                                    nguvl = CFrame.new(MyRealEnemies.Character.HumanoidRootPart.Position + MyRealEnemies.Character.HumanoidRootPart.CFrame.LookVector * posi, MyRealEnemies.Character.HumanoidRootPart.Position)*CFrame.new(0,yTween,0)
                                    Di(nguvl*CFrame.new(MyRealEnemies.Character.HumanoidRootPart.Position.Unit))
                                else
                                    nguvl = CFrame.new(MyRealEnemies.Character.HumanoidRootPart.Position + MyRealEnemies.Character.HumanoidRootPart.CFrame.LookVector * 5, MyRealEnemies.Character.HumanoidRootPart.Position)*CFrame.new(0,5,0)
                                    Di(nguvl)
                                end
                            else 
                                print("Player Died")
                                SkipPlayer()
                            end
                        else
                            if MyRealEnemies.Character.Humanoid.Health > 0 then
                                Di(MyRealEnemies.Character.HumanoidRootPart.CFrame*CFrame.new(0,10,0))
                            else
                                print("Player Died")
                                SkipPlayer()
                            end
                        end
                    else
                        SkipPlayer()
                    end
                end)
                a = MyRealEnemies.Character.HumanoidRootPart.Position
                if a ~= b then
                    yTween = 0
                    b = a
                    if (getgenv().Setting.Gun.Enable and getgenv().Setting.Gun.GunMode) then
                        posi = 14
                    else
                        posi = 15
                    end
                else
                    yTween = 5
                    if (getgenv().Setting.Gun.Enable and getgenv().Setting.Gun.GunMode) then
                        posi = 3
                    else
                        posi = 5
                    end
                end
                if MyRealEnemies.Character.HumanoidRootPart.CFrame.Y >= 10 then
                    cocc = true
                else
                    cocc = false
                end
            else
                safehealth = true
                Di(MyRealEnemies.Character.HumanoidRootPart.CFrame*CFrame.new(0,math.random(5000,100000),0))
            end
        end
    end
end)
--- Aim
spawn(function()
    while task.wait() do 
        if MyRealEnemies ~= nil and (MyRealEnemies.Character:WaitForChild("HumanoidRootPart").CFrame.Position - game.Players.LocalPlayer.Character:WaitForChild("HumanoidRootPart").CFrame.Position).Magnitude < 40 then 
            aim = true 
            if (getgenv().Setting.Gun.Enable and getgenv().Setting.Gun.GunMode) then
                CFrameHunt = CFrame.new(MyRealEnemies.Character.HumanoidRootPart.Position + MyRealEnemies.Character.HumanoidRootPart.CFrame.LookVector * 2, MyRealEnemies.Character.HumanoidRootPart.Position)
            else
                CFrameHunt = CFrame.new(MyRealEnemies.Character.HumanoidRootPart.Position + MyRealEnemies.Character.HumanoidRootPart.CFrame.LookVector * 5, MyRealEnemies.Character.HumanoidRootPart.Position)
            end
        else
            aim = false
        end
    end
end)
spawn(function()
    local gg = getrawmetatable(game)
    local old = gg.__namecall
    setreadonly(gg,false)
    gg.__namecall = newcclosure(function(...)
        local method = getnamecallmethod()
        local args = {...}
        if tostring(method) == "FireServer" then
            if tostring(args[1]) == "RemoteEvent" then
                if tostring(args[2]) ~= "true" and tostring(args[2]) ~= "false" then
                    if aim then
                        args[2] = CFrameHunt.Position
                        return old(unpack(args))
                    end
                end
            end
        end
        return old(...)
    end)
end)
local Mouse = lp:GetMouse()
Mouse.Button1Down:Connect(function()
    pcall(function()
        if MyRealEnemies ~= nil and aim then
            local args = {
                [1] = CFrameHunt.Position,
                [2] = MyRealEnemies.Character.HumanoidRootPart
            }
            game:GetService("Players").LocalPlayer.Character[game.Players.LocalPlayer.Character:FindFirstChildOfClass("Tool").Name].RemoteFunctionShoot:InvokeServer(unpack(args))
        end
    end)
end)
spawn(function()
    while task.wait() do
        local targetPlayer = MyRealEnemies
        if targetPlayer ~= nil then
            local targetCharacter = targetPlayer.Character
            local camera = game.Workspace.CurrentCamera
            if targetCharacter then
                local lookAtPos = targetCharacter.HumanoidRootPart.Position
                local cameraPos = camera.CFrame.Position
                local newLookAt = CFrame.new(cameraPos, lookAtPos)
                camera.CFrame = newLookAt
            end
        end
    end
end)
