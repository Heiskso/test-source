function GetElapsedTime(startTime)
    local elapsedTime = tick() - startTime
    local hours = math.floor(elapsedTime / 3600)
    local minutes = math.floor((elapsedTime % 3600) / 60)
    local seconds = math.floor(elapsedTime % 60)
    local formattedTime = string.format("%02d:%02d:%02d", hours, minutes, seconds)
    return formattedTime
end
function AddBounty()
    Earned = game:GetService("Players").LocalPlayer.leaderstats["Bounty/Honor"].value
    CurrentBounty.Text = "Current Bounty:"tostring(Earned)
end
task.spawn(function()
    while wait(1) do
        TimeEslaped.Text = GetElapsedTime(startTime)
        TgetNamechucjnua.Text = "Target Name: "..MyRealEnemies.Name
        AddBounty()
        saveSettings()
    end
end)
