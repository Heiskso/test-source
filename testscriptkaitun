			ColorSelection.Size = UDim2.new(0, 18, 0, 18)
			ColorSelection.ZIndex = 25
			ColorSelection.Image = "rbxassetid://4953646208"
			ColorSelection.ScaleType = Enum.ScaleType.Fit

